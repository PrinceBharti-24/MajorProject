<h1>MajorProject /<%= title%></h1>
<section id="feed-post">
  <h4>Posts</h4>
  <% if(locals.user){%>
  <form action="/posts/create" method="post">
    <textarea name="content" cols="30" rows="3" required> </textarea>
    <input type="submit" value="Post" />
  </form>
  <%} %>
  <div class="container-list">
    <ul>
      <% for(post of posts){%>
        <%- include('_post.ejs')-%>
      <%} %>
    </ul>
  </div>
</section>
